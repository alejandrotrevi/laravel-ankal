{
    "name": "alejandrotrevi/laravel-ankal",
    "description": "Super simple statuses for your Laravel Models",
    "keywords": [
        "alejandrotrevi",
        "laravel-ankal"
    ],
    "homepage": "https://github.com/alejandrotrevi/laravel-ankal",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Alejandro",
            "email": "alex_tf_1992@live.com.mx",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "~5.8.0|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0|^9.3",
        "orchestra/testbench": "~3.8.0|^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Alejandrotrevi\\LaravelAnkal\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alejandrotrevi\\LaravelAnkal\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alejandrotrevi\\LaravelAnkal\\LaravelAnkalServiceProvider"
            ],
            "aliases": {
                "LaravelAnkal": "Alejandrotrevi\\LaravelAnkal\\LaravelAnkalFacade"
            }
        }
    }
}
